<script>
    $(".dmleiv2-medias-items li").eq(0).hover(function(){
        $(".dmleiv2-medias-items li").removeClass("this");
        $(this).addClass("this");
        $(".dmleiv2-medias-bg").stop();
        $(".dmleiv2-medias-bg").css({'left':'-400'+'px'});
        $(".dmleiv2-medias-box").hide().eq($('.dmleiv2-medias-items li').index(this)).show();
    });
    $(".dmleiv2-medias-items li").eq(1).hover(function(){
        $(".dmleiv2-medias-items li").removeClass("this");
        $(this).addClass("this");
        $(".dmleiv2-medias-bg").stop();
        $(".dmleiv2-medias-bg").css({'left':'0'});
        $(".dmleiv2-medias-box").hide().eq($('.dmleiv2-medias-items li').index(this)).show();
    });
    $(".dmleiv2-medias-items li").eq(2).hover(function(){
        $(".dmleiv2-medias-items li").removeClass("this");
        $(this).addClass("this");
        $(".dmleiv2-medias-bg").stop();
        $(".dmleiv2-medias-bg").css({'left':'400'+'px'});
        $(".dmleiv2-medias-box").hide().eq($('.dmleiv2-medias-items li').index(this)).show();
    });
    $(".dmleiv2-medias-items li").eq(1).hover();
    $('.thum-item').hover(
            function(event) {
                var circle = $(this).find('.thum-item-circle');
                circle.attr('style', '');
                $(this).addClass('active');
            },
            function(event) {
                var circle = $(this).find('.thum-item-circle');
                circle.css({
                    transition: 'none',
                    transform: 'scale(1.2)',
                    top: '-15px',
                    borderRadius: '50%'
                });
                setTimeout(function() {
                    circle.css({
                        transition: 'all .15s ease',
                        transform: 'scale(0.5)',
                        top: '-15px',
                        height: '267px',
                        borderRadius: '50%'
                    });
                }, 0);
                $(this).removeClass('active');
            }
    );
</script>
