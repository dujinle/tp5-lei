<script>
    //微信扫一扫登录
    $("#wx-scan").click(function(){
        $('#vercode').attr('placeholder','请输入微信验证码');
        $(".user-wx").addClass('user-wx-show');
        $("#wxregister-text").css('display','none');
        $("#btnverycode").css('display','none');
        $("#phone-register").css('display','block');
    })
    $("#phone-register").click(function(){
        $('#vercode').attr('placeholder','请输入短信验证码');
        $(".user-wx").removeClass('user-wx-show');
        $("#wxregister-text").css('display','block');
        $("#btnverycode").css('display','block');
        $("#phone-register").css('display','none');
    })

     function changeVerify(){
        $('.verify').attr('src',"/Misc/verify?num="+Math.floor(Math.random()*(1000+1)));
    }
